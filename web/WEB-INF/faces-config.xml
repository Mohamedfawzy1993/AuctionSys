<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
    <converter>
        <converter-for-class>model.dto.UserSellProduct</converter-for-class>
        <converter-class>ddd.UserSellProductConverter</converter-class>
    </converter>
    <converter>
        <converter-for-class>model.dto.UserBidProduct</converter-for-class>
        <converter-class>ddd.UserBidProductConverter</converter-class>
    </converter>
    <converter>
        <converter-for-class>model.dto.Auction</converter-for-class>
        <converter-class>ddd.AuctionConverter</converter-class>
    </converter>
    <converter>
        <converter-for-class>model.dto.User</converter-for-class>
        <converter-class>ddd.UserConverter</converter-class>
    </converter>
    <converter>
        <converter-for-class>model.dto.Product</converter-for-class>
        <converter-class>ddd.ProductConverter</converter-class>
    </converter>
    <converter>
        <converter-for-class>model.dto.ProductCategory</converter-for-class>
        <converter-class>ddd.ProductCategoryConverter</converter-class>
    </converter>
    <managed-bean>
        <managed-bean-name>userSellProduct</managed-bean-name>
        <managed-bean-class>ddd.UserSellProductManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>userBidProduct</managed-bean-name>
        <managed-bean-class>ddd.UserBidProductManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>auction</managed-bean-name>
        <managed-bean-class>ddd.AuctionManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>user</managed-bean-name>
        <managed-bean-class>ddd.UserManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>product</managed-bean-name>
        <managed-bean-class>ddd.ProductManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>productCategory</managed-bean-name>
        <managed-bean-class>ddd.ProductCategoryManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>editUserSellProduct</from-outcome>
            <to-view-id>/ddd/EditUserSellProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewUserSellProduct</from-outcome>
            <to-view-id>/ddd/ViewUserSellProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userSellProductList</from-outcome>
            <to-view-id>/ddd/ListUserSellProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createUserSellProduct</from-outcome>
            <to-view-id>/ddd/CreateUserSellProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editUserBidProduct</from-outcome>
            <to-view-id>/ddd/EditUserBidProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewUserBidProduct</from-outcome>
            <to-view-id>/ddd/ViewUserBidProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userBidProductList</from-outcome>
            <to-view-id>/ddd/ListUserBidProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createUserBidProduct</from-outcome>
            <to-view-id>/ddd/CreateUserBidProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editAuction</from-outcome>
            <to-view-id>/ddd/EditAuction.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewAuction</from-outcome>
            <to-view-id>/ddd/ViewAuction.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>auctionList</from-outcome>
            <to-view-id>/ddd/ListAuction.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createAuction</from-outcome>
            <to-view-id>/ddd/CreateAuction.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editUser</from-outcome>
            <to-view-id>/ddd/EditUser.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewUser</from-outcome>
            <to-view-id>/ddd/ViewUser.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userList</from-outcome>
            <to-view-id>/ddd/ListUser.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createUser</from-outcome>
            <to-view-id>/ddd/CreateUser.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editProduct</from-outcome>
            <to-view-id>/ddd/EditProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewProduct</from-outcome>
            <to-view-id>/ddd/ViewProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>productList</from-outcome>
            <to-view-id>/ddd/ListProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createProduct</from-outcome>
            <to-view-id>/ddd/CreateProduct.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editProductCategory</from-outcome>
            <to-view-id>/ddd/EditProductCategory.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewProductCategory</from-outcome>
            <to-view-id>/ddd/ViewProductCategory.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>productCategoryList</from-outcome>
            <to-view-id>/ddd/ListProductCategory.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createProductCategory</from-outcome>
            <to-view-id>/ddd/CreateProductCategory.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>